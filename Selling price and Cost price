#include<stdio.h>
int main(){
    int cp,sp;
    printf("enter cp and sp: ");
    
    scanf("%d %d",&cp,&sp);
    if(cp<sp){
        printf("seller made profit:");
    }
    if(cp>sp){
        printf("seller made loss");
    }
    if(cp==sp){
         printf("no profit,no loss");
    }
    return 0;
}

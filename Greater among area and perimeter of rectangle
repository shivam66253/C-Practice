#include <stdio.h>
int main()
{
    int l;
    printf("enter length : ");
    scanf("%d", &l);
    int b;
    printf("enter breadth :");
    scanf("%d", &b);
    int A = l * b;
    int P = 2 * (l + b);
    if (A > P)
    {
        printf("area is greater than perimeter: ");
    }
    else
    {
        printf("area is less than perimeter: ");
    }

    return 0;
}

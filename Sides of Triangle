#include<stdio.h>
int main(){
    int a,b,c;
    printf("enter a ");
    scanf("%d",&a);
    printf("enter b ");
    scanf("%d",&b);
    printf("enter c ");
    scanf("%d",&c);
    if((a+b)>c && (b+c)>a && (a+c)>b){// sum of two sides should be greater than the third one
        printf("yes they can be sides of a triangle ");
    }
    else{
        printf("no they can't be sides of a triangle ");
    }
    
    return 0;

#include<stdio.h>
int main() { 
    int a,b,c;// a=ram,b=shyam,c=ajay
    printf("enter 1st age: ");
    scanf("%d",&a);
    printf("enter 2nd age: ");
    scanf("%d",&b);
    printf("enter 3rd age: ");
    scanf("%d",&c);
    if(a>b){
        if(a>c)
        printf("%d is greatest",a);
        else
        printf("%d is greatest",c);
    }
    else{
        if(b>c)
        printf("%d is greatest",b);
        else
        printf("%d is greatest",c);

    }
    return 0;
}
